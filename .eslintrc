{
	"env": {
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"indent": "off",
		"no-tabs": "off",

		"@typescript-eslint/indent": ["error", "tab", { "SwitchCase": 1 }]
	}
}
